<!DOCTYPE html>
<html>
  <head>
    <title>Generic Partner Example App</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>Users</h1>
    <% users.forEach((user) => { %>
      <div style="padding-bottom: 1rem; border-bottom: 1px solid #000;">
        <p><%= user.id %>: <%= user.firstName %> <%= user.lastName %> (<%= user.email %>)</p>
        <p>User is <%= user.apiKey ? 'connected' : 'disconnected' %></p>
        <a href="/login/<%= user.id %>">Login as this user</a>
      </div>
    <% }) %>
  </body>
</html>
